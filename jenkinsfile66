pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                echo 'Hello World'
            }
        }
         stage('compile') {
            steps {
                bat 'javac Helloworld.java'
            }
        }
           stage('run') {
            steps {
                bat 'java Helloworld'
            }
        }
    }
}
