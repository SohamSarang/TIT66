pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', credentialsId: '21ab8f43-f6b1-4d3a-b0c4-16fc14ab899e', url: 'https://github.com/SohamSarang/TIT66.git'
            }
        }
         stage('compile') {
            steps {
                bat 'javac Helloworld.java'
            }
        }
           stage('run') {
            steps {
                bat 'java Helloworld'
            }
        }
    }
}
